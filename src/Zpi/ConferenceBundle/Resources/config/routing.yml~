registration_new:
    pattern:  /conference/registration
    defaults: { _controller: ZpiConferenceBundle:Registration:new }
    
conference_new:
    pattern:  /conference/new
    defaults: { _controller: ZpiConferenceBundle:Conference:new }
    
conference_show:
    pattern:  /conference/{id}
    defaults: { _controller: ZpiConferenceBundle:Conference:show }
    requirements: 
          id: \d+
    
conference_list:
    pattern:  /conference/list
    defaults:  { _controller: ZpiConferenceBundle:Conference:menu }
    
registration_delet:
    pattern:  /conference/registration/delete/{id}
    defaults:  { _controller: ZpiConferenceBundle:Registration:delete }
    
